{
	"name": "segfaultapi",
	"version": "1.0.0",
	"description": "Web utilities and APIs for Project Segfault, written in TypeScript with express.",
	"scripts": {
		"dev": "tsup --watch --onSuccess \"cp -r public/**/* dist && pnpm preview\"",
		"preview": "node dist/index.js",
		"build": "rm -rf dist && tsup && cp -r public/**/* dist",
		"format": "prettier --write .",
		"docker": "docker compose up"
	},
	"author": "Project Segfault team",
	"license": "MIT",
	"dependencies": {
		"@fastify/formbody": "^7.3.0",
		"@fastify/sensible": "^5.1.1",
		"@fastify/static": "^6.5.0",
		"discord-webhook-node": "^1.1.8",
		"dotenv": "^16.0.3",
		"fastify": "^4.9.2",
		"hcaptcha": "^0.1.1"
	},
	"type": "module",
	"devDependencies": {
		"@types/express": "^4.17.14",
		"@types/node": "^18.11.9",
		"prettier": "^2.7.1",
		"tsup": "^6.4.0"
	}
}
